scp chen-huang-zhang@210.45.78.184:~/transfer/2D/J2_0.50/L*.zip .

read -p "System size :" L
read -p "Beta :" Beta
read -p "lowest order :" Order1
read -p "highest order :" Order2

for ((i=$Order1; i<=$Order2; i++))
do
    rm -r L"$L"_"$Beta"_"$i"
    unzip L"$L"_"$Beta"_"$i".zip
    rm L"$L"_"$Beta"_"$i".zip
done
